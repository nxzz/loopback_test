{
  "extends": [
    "eslint:recommended",
    // "loopback"
  ],
  "rules": {
    "semi": "error",
    "no-unused-vars": 0,
    "angular/no-services": 0,
    "no-console": 0
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "io": true
  }
}